{
  "manifest_version": 3,

  "name": "Wikit",
  "description": "Search Wikipedia on the go!",
  "version": "1.0",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },

  "background": {
    "service_worker": "eventPage.js",
    "type": "module"
  },

  "permissions": [  
          "tabs",      
          "contextMenus",
          "activeTab"
        ]
}